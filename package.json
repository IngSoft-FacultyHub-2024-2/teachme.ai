{
  "name": "teachme-ai",
  "version": "1.0.0",
  "description": "Console-based TypeScript application",
  "main": "dist/index.js",
  "bin": {
    "teachme": "./dist/index.js"
  },
  "preferGlobal": true,
  "files": [
    "dist/",
    "src/inputData/",
    ".env.example",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "watch": "tsc --watch",
    "package": "npm run build && npm pack",
    "prerelease": "npm run lint && npm run type-check && npm test",
    "release:major": "npm run prerelease && npm version major",
    "release:minor": "npm run prerelease && npm version minor",
    "release:patch": "npm run prerelease && npm version patch",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/figlet": "^1.7.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@types/inquirer": "^9.0.0",
    "@types/ora": "^3.1.0",
    "chalk": "^4.1.2",
    "dotenv": "^16.4.0",
    "figlet": "^1.9.4",
    "inquirer": "^8.2.5",
    "openai": "^6.3.0",
    "ora": "^5.4.1"
  }
}
